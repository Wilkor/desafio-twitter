<div class="card" style="width: 16rem;">
    <img class="card-img-top" src="{{this.user.profile_image_url}}" alt="Card image cap">
     <div class="card-body">
    <h5 class="card-title">Nome: {{this.user.name}}
        <hr>
    </h5>
    <p class="card-text">Tweet:{{this.text}}</p>
  <a href="/search-user?hash=@{{this.user.screen_name}}"  class="btn btn-primary">Perfil</a>
  </div>
</div>
<br>

